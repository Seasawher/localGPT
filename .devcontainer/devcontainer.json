{
    "name": "local-gpt-dev",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "local_gpt",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "settings": {
                "editor.formatOnSave": true,
                "editor.insertSpaces": true,
                "files.eol": "\n",
                "files.trimTrailingWhitespace": true
            },
            "extensions": [
				"mhutchie.git-graph",
				"eamodio.gitlens",
				"ms-azuretools.vscode-docker",
				"tomoki1207.pdf"
			]
        }
    }
}